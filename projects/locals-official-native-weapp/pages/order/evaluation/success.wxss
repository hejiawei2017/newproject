.container {
	position: relative;
	box-sizing: border-box;
	width: 690rpx;
	height: 463rpx;
	background-color: #ffffff;
	box-shadow: 0px 4px 38px 0px rgba(39, 39, 39, 0.09);
	border-radius: 10px;
	margin: 70rpx auto 0 auto;
	padding-top: 95rpx;
	text-align: center;
}

.title {
	font-size: 36rpx;
	font-weight: 400;
	line-height: 29rpx;
	letter-spacing: 0px;
	color: #484848;
}
super-image.success {
	position: absolute;
	top: -54rpx;
	left: 50%;
	transform: translateX(-50%);
}
super-image.success image {
	width: 103rpx;
	height: 111rpx;
}

.descript {
	font-size: 27rpx;
	font-weight: 300;
	line-height: 40rpx;
	letter-spacing: 0px;
	color: #999999;
	margin-top: 30rpx;
	white-space: pre;
}

.footer {
	position: absolute;
	bottom: 49rpx;
	height: 80rpx;
	line-height: 80rpx;
	left: 0;
	right: 0;
	display: flex;
	flex-flow: row nowrap;
	justify-content: space-between;
	padding: 0 40rpx;
	font-size: 28rpx;
	font-weight: 300;
	text-align: center;
}

.footer .footer-left {
	width: 327rpx;
	border-radius: 12rpx;
	border: solid 1px #e84358;
	color: #e84358;
  background: white;
}

.footer .footer-right {
	width: 327rpx;
	background-color: #e84358;
	border-radius: 12rpx;
	color: #ffffff;
}

super-image.coupon50 image {
  margin-top: 30rpx;
  width: 470rpx;
  height: 140rpx;
}

super-image.business-vip-banner image {
	display: block;
	width: 690rpx;
  height: 403rpx;
  margin: 40rpx auto;
}