.vip-image-wrap {
  position: fixed;
  z-index: 10000;
  bottom: 0;
  left: 0;
  margin-bottom: 130rpx;
  padding: 0 0 30rpx 0;
  transition: all 0.5s ease;
  transform: translateX(-100%);
}
.vip-image-wrap image {
  width: 94rpx;
  height: 50rpx;
}
.vip__wrap {
  position: fixed;
  z-index: 10000;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-top: 1px solid rgba(0, 0, 0, 0.06);
  margin-bottom: 130rpx;
  height: 102rpx;
  left: 0;
  right: 0;
  bottom: 0;
  background: white;
  padding: 0 50rpx 30rpx 50rpx;
  transition: all 0.5s ease;
  transform: translateX(-100%);
}
.show-buy-vip {
  transform: translateX(0);
}
.show-buy-image {
