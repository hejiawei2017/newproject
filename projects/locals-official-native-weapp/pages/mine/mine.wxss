  padding-top: 20rpx;
  padding-left: 10rpx;
}

@keyframes cardpop {
  from {
    bottom: -5rpx;
  }
  to {
    bottom: 10rpx;
  }
}

.cardanimate {
  animation-name: cardpop;
  animation-duration: 0.3s;
  animation-timing-function: ease-out; /* or: ease, ease-in, ease-in-out, linear, cubic-bezier(x1, y1, x2, y2) */
  animation-fill-mode: forwards;
  animation-delay: 1s;
}

