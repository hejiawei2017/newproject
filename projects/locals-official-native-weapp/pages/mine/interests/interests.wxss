
page{
  background: #fff;
}
.interests-til{
  background: #fff;
}
.message{
    padding:30rpx 40rpx;
    position: relative;
    display: flex;
    align-items: center;
}
.message .message-level{
    flex:5;
}
.message .message-level>text{
  font-size:42rpx;
}
.message .message-level .nessage-progress{
  margin:20rpx 0;
  vertical-align: middle;
}
.message .message-level .nessage-progress image{
  width:26rpx;
  vertical-align: middle;
}
.message .message-level .nessage-progress text{
  color:rgb(68,68,68);
  font-size:24rpx;
  vertical-align: middle;
  margin-left:12rpx;
}
.message .message-head{
  flex:2;
}
.message  .message-head image{
  width:100rpx;
  float:right;
  height:100rpx;
  border-radius:100rpx;
  border:1px solid #ccc;
}
.memberInte{
  padding:20rpx 40rpx;
  background-color: "#ffffff";
  box-shadow: 0px 0px 14rpx 2rpx rgba(69, 69, 69, 0.1);
}
.memberInte text{
  font-size:32rpx;
}
.levelList{
  width:100%;
  margin-top:20rpx;
  position:relative;
  height:146rpx;
}
.levelList .levelLine{
  width:100%;
  border-top:2px solid #ccc;
  position:absolute;
  top:14rpx;
  left:0;
  z-index: 8;
}
.levelList .levelLineAvtive{
  width: 0%;
  border-top:2px solid #949494;
  position:absolute;
  top:14rpx;
  left:0;
  z-index: 8;
}
.levelList .levelLineAvtive.NORMAL{
  width: 0%;
}
.levelList .levelLineAvtive.SILVER{
  width: 33.3%;
}
.levelList .levelLineAvtive.GOLD{
  width: 66.6%;
}
.levelList .levelLineAvtive.BLACK{
  width: 100%;
}
.levelList .levelDes{
  display:inline-block;
  text-align:center;
  background: #fff;
  width: 70rpx;
  text-align: center;
  z-index: 9;
}
.levelList .levelDes image{
  width:26rpx;
}
.levelList .levelDes text{
  display:block;
  font-size:18rpx;
  /* line-height:24px; */
  color:#999;
  white-space:nowrap;
}
.levelList .levelDes text:nth-child(3){
  /* display: none; */
  font-size:20rpx;
  margin-left: -26%;
  line-height:52rpx;
}
.levelList .levelDes.active text{
  color:#444;
}
.levelList .levelDes.active text:nth-child(3){
  display: block;
}
.levelList .levelDes:nth-child(1){
  position:absolute;
  top:0;
  left:0;
  overflow:hidden;
  height:100%;
}
.levelList .levelDes:nth-child(2){
  position:absolute;
  top:0;
  left:33.3%;
  transform: translateX(-50%);
}
.levelList .levelDes:nth-child(3){
  position:absolute;
  left:66.6%;
  transform: translateX(-50%);
}
.levelList .levelDes:nth-child(4){
  position:absolute;
  top:0;
  right:0;
}
.interests-int{
  margin-top:20rpx;
  padding:30rpx 40rpx;
  background: #fff;
}
.rules{
  width:100%;
  padding-bottom:20rpx;
}
.rules text{
  display: block;
  font-size:32rpx;
  line-height:36px;
  color:black;
}
.rules .textarea{
  font-size:24rpx;
  line-height:38rpx;
  color:#666;
  font-weight:500;
}

.interests-int .weui-cell {
  padding:26rpx 0;
  position:static;
  display:-webkit-box;
  display:-webkit-flex;
  display:flex;
  -webkit-box-align:center;
  -webkit-align-items:center;
  align-items:center;
  border-bottom:1px solid #ccc;
}
.interests-int .weui-cell.title{
  background: #e84358;
  padding:20rpx 0;
}
.interests-int .weui-cell.title p{
  color: #fff;
}
.interests-int .weui-cell .weui-cell__hd{
  text-align: center;
}
.interests-int .weui-cell .weui-cell__bd{
  /* text-align: center; */
}
.interests-int .weui-cell .weui-cell__ft{
  text-align: center;
}
.weui-cell__hd{
  -webkit-box-flex:3;
  -webkit-flex:3;
  flex:3;
  vertical-align: middle;
}
.weui-cell__hd p{
  font-size:24rpx;
  vertical-align: middle;
}
.weui-cell__hd .cellimg{
  width:26rpx;
  vertical-align: middle;
  float:left;
  margin-top:8rpx;
}
.weui-cell__bd {
  -webkit-box-flex:4;
  -webkit-flex:4;
  flex:4;
  font-size:28rpx;
  color:#444;
}
.weui-cell__bd p{
  font-size:24rpx;
  text-align:left;
  text-align:center;
  display:block;
}
.weui-cell__ft {
  text-align:left;
  color:#444;
  font-size:28rpx;
  line-height:20px;
  flex:2;
  -webkit-box-flex:2;
  -webkit-flex:2;
}
.weui-cell__ft p{
  font-size:24rpx;
}
.conditions{
  width:100%;
  padding-top:10rpx;
}
.conditions text{
  font-size:32rpx;
  line-height:40px;
  color:black;
  display:block;
}
.conditions .up{
  font-size: 24rpx;
	line-height: 38rpx;
	color: #666666;
}