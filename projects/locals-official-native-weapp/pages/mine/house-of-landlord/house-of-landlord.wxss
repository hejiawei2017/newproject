.house-info .house-image super-image image {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}

.house-info:last-child .detail {
  border-bottom: none;
}

.house-info .detail {
  position: relative;
  font-family: PingFangSC-Regular;
  margin-left: 27rpx;
  height: 100%;
}

.house-info .detail view:nth-of-type(1) {
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  font-size: 32rpx;
  line-height: 42rpx;
  font-weight: 300;
  color: #484848;
}

.house-info .detail > view:nth-of-type(2) {
  margin-top: 20rpx;
  font-size: 24rpx;
  color: #999999;
  line-height: 18rpx;
}

.house-info .detail > view:nth-of-type(3) {
  position: absolute;
  left: 0;
  bottom: 33rpx;
  font-size: 32rpx;
  font-weight: 500;
  color: #fc2b53;
  line-height: 28rpx;
}

.house-info .detail > view:nth-of-type(4) {
  position: absolute;
  right: 0;
  bottom: 28rpx;
  width: 90rpx;
  height: 40rpx;
  line-height: 40rpx;
  border: 1rpx solid #e84358;
  border-radius: 10rpx;
  font-size: 22rpx;
  color: #e84358;
  text-align: center;
}
