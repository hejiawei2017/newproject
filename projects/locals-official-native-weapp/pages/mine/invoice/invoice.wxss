.bottom{
    width:20px;
    height:20px;
    position:absolute;
    left:50%;
    top:50%;
    transform: translateX(46px) translateY(-50%);
    margin-top:6px;
    vertical-align: middle;
    z-index: 2;/*兼容ie8-*/
}
.bottom-arrow1,.bottom-arrow2{
    width:0;
    height:0;
    display:block;
    position:absolute;
    left:0;
    top:0;
    z-index: 5;/*兼容ie8-*/
    border-bottom:6px transparent dashed;
    border-left:6px transparent dashed;
    border-right:6px transparent dashed;
    border-top:6px white solid;
    overflow:hidden;
}
.bottom-arrow1{
    top:2px;/*重要*/
    border-top:6px #e84358 solid;
}
.bottom-arrow2{
    border-top:6px white solid;
}
.top{
    width:20px;
    height:20px;
     position:absolute;
    left:50%;
    top:50%;
    transform: translateX(46px) translateY(-50%);
    margin-top:2px;
    z-index: 2;/*兼容ie8-*/
}
.top-arrow1,.top-arrow2{
    width:0;
    height:0;
    display:block;
    position:absolute;
    left:0;
    top:0;
    z-index: 5;/*兼容ie8-*/
    border-top:6px transparent dashed;
    border-left:6px transparent dashed;
    border-right:6px transparent dashed;
    border-bottom:6px white solid;
    overflow:hidden;
}
.top-arrow1{
    border-bottom:6px #e84358 solid;
}
.top-arrow2{
    top:2px;/*重要*/
    border-bottom:6px white solid;
}
