/* pages/mine/invoice/invoice.wxss */
.invoice{
  padding:20rpx 40rpx;
  margin-bottom: 130rpx;
}
.invoice-title{
  font-size:24rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(153,153,153,1);
  text-align: center;
  display: block;
  margin-top: 56rpx;
}
.invoice-title image{
  width:20px;
  height:auto;
  margin-left:4px;
  vertical-align:middle;
}
.invoice-messa-wrap{
  height: 40rpx;
  overflow: hidden;
  margin-bottom: 53rpx;
}
.invoice-messa{
  height: 40rpx;
  line-height: 40rpx;
  float:right;
  margin-top:3px;
  font-family: "PingFangSC-Regular";
  font-size: 28rpx;
  color: #E84358;
}
.invoice-cont{
  margin:27rpx 0;
  padding:30rpx 30rpx 30rpx 30rpx;
  font-size: 13rpx;
  position: relative;
  background:rgba(255,255,255,1);
  box-shadow:0px 7px 21px 0px rgba(0, 0, 0, 0.09);
  border-radius:10rpx;
}
.left-roll,.right-roll{
  position: absolute;
  background: #fff;
  width: 13rpx;
  height: 26rpx;
  
}
.left-roll{
  top: 78rpx;
  left: -32rpx;
  border-radius: 0 13rpx 13rpx 0;
  box-shadow: -2rpx 0px 2rpx rgba(69, 69, 69, 0.2) inset;
}
.right-roll{
  top: 78rpx;
  right:-32rpx;
  border-radius: 13rpx 0 0 13rpx;
  box-shadow: 2rpx 0px 2rpx rgba(69, 69, 69, 0.2) inset;
}
.weui-cells::before{
  content: '';
  border: 0;
}
.invoice-cont .order-detail{
  font-size: 11px;
}
.weui-cell {
  padding:30rpx 0px;
  position:relative;
  display:-webkit-box;
  display:-webkit-flex;
  display:flex;
  -webkit-box-align:center;
  -webkit-align-items:center;
  align-items:center;
  font-family: 'PingFangSC-Regular';
  font-size: 13px;
  font-weight: 400;
}
.weui-cells.hidden , .weui-cell.hidden{
  display: none;
}
.weui-cell:before {
    content: " ";
    position: absolute;
    top: 0;
    right: 0;
    height: 1px;
    border-top: 1px solid #ccc;
    color: #ccc;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
    left: 0px;
    z-index: 2;
}
.weui-cells.orders{
  padding:15rpx 0 15rpx 0;
  margin-top: 0;
}
.weui-cells.orders .weui-cell:before {
    content: " ";
    position: absolute;
    top: 0;
    right: 0;
    height: 0px;
    border-top: 0px solid #ccc;
    color: #ccc;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
    left: 0px;
    z-index: 2;
}
.weui-cells.orders .weui-cell{
  padding:15rpx 0;
  padding-top:30rpx;
}
.weui-cells.orders .weui-cell.cell{
  padding-top:15rpx;
  padding-bottom:30rpx;
  border-bottom:1px solid #ccc;
}
.invoice-cont .instatus{
  padding:30rpx 0;
}
.invoice-cont .instatus text{
  font-size:14px;
}
.invoice-cont .instatus .status{
  font-size: 14px;
}
.electronic{
  display:inline-block;
  font-size:14px;
  padding:9rpx 30rpx 9rpx 30rpx;
  background: #e84358;
  color:#fff;
  border-top-right-radius: 40rpx;
  border-bottom-right-radius: 40rpx;
  margin-left:-30rpx;
}
