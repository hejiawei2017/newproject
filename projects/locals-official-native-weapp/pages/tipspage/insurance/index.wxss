/* pages/tipspage/insurance/index.wxss */
.page{
  padding: 0 20px;
}
.title{
  font-size: 15px;
  font-weight: bold;
  padding: 0 0 30px 0;
  position: relative;
}
.section{
  position: relative;
  padding: 20px 0;
}
.section-title{
  font-size: 15px;
  font-weight: bold;
  margin-bottom: 20px;
}
.table{
  border: 1px solid #e84358;
  border-bottom: 0;
  font-size: 13px;
  font-weight: 200;
  color: #666;
  background-color: #fce7e9;
}
.tr{
  display: flex;
  border-bottom: 1px solid #e84358;
}
.td{
  min-height: 40px;
  display: flex;
  align-items: center;
}
.tr .td:nth-child(1){
  width: 70%;
  border-right: 1px solid #e84358;
  padding-left: 10px;
}
.tr .td:nth-child(2){
  width: 30%;
  justify-content: center;
}
.section-content{
  font-size: 13px;
  color: #666;
}