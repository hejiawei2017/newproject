tag {
  color: #e84358;
}
.tags-container {
  margin: 0 30rpx;
}

.keyword-scroll {
  position: absolute;
  top: 110rpx;
  bottom: 0;
  left: 0;
  right: 0;
  box-sizing: border-box;
}
.weui-media-box__desc {
  line-height: 22px;
}
.rect40 {
  width: 35px;
  height: 35px;
}
.weui-media-box__title {
  font-size: 30rpx;
  font-weight: 300;
}
.weui-media-box__desc {
  display: flex;
  justify-content: space-between;
  font-weight: 300;
}
.weui-media-box {
  padding: 30rpx 0;
  border-bottom: 1px solid #f8f8f8;
  margin: 0 40rpx;
}
.weui-media-box::before {
  border: none;
}
.weui-media-box_price {
  float: right;
  font-size: 22px;
  color: rgb(255, 133, 133);
}
.notText{
  font-size:14px;
  text-align: center;
  padding:20% 0;
}

.keyword-color {
  color: #e84358; 
}

.search-title {
  display: inline-block;
  
  width: 80%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.title {
  background: #f8f8f8;
  font-size: 26rpx;
  font-weight: 300;
  padding: 10rpx 40rpx;
}

.see-all {
  float: right;
  color: #e84358;
}

.money {
  float: right;
  color: #e84358;
}

.address {
  display: block;
  width: 450rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.error-container {
  padding: 30rpx 45rpx;
  font-size: 28rpx;
  font-weight: 300;
}

.empty-house-list {
  color: #999999;
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-content: center;
  line-height: 130rpx;
  padding: 0 40rpx;
  border-radius: 20rpx;
  box-shadow: 0 0 20rpx #e9e9e9;
}

.empty-house-list icon {
  display: block;
  align-self: center;
}

.empty-house-list text {
  display:block;
  margin:0 auto;
}