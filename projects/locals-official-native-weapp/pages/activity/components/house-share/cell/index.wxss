.cell {
    width: 100%;
    /* height: 108rpx; */
    display: flex;
    padding: 10rpx 0;
    margin: 20rpx 0;
}

.cell .cell--left {
    /* width: 120rpx; */
    height: 100%;
}

.cell .cell--left image {
    width: 88rpx;
    height: 88rpx;
    margin: 0rpx 40rpx 0rpx 0;
    border-radius: 100%;
}

.cell .cell--center {
    width: 0rpx;
    padding: 0;
    flex: 1;
    padding: 8rpx 0;
}

.cell .cell--center .content {
    width: 400rpx;
    font-size:28rpx;
    font-weight:400;
    color:rgba(67,67,67,1);
    line-height:36rpx;
}

.cell .cell--right {
    flex: 1;
    padding: 8rpx 0;
    width: 130rpx;
}

.cell .cell--right .time {
    font-size:28rpx;
    font-weight:300;
    color:rgba(155,155,155,1);
    text-align: right;
    line-height:36rpx;
}