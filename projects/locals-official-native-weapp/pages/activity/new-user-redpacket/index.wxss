.page-container {
    background-color: #f3d0cc;
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
    height: 100%;
    width: 100%;
}

.banner-image {
    height: 509rpx;
    width: 100%;
    display: block;
    overflow: hidden;
}

.form {
    background: #fff;
    margin: 0 38rpx;
    height: 445rpx;
    box-sizing: border-box;
    padding: 35rpx 72rpx;
    border-radius: 11rpx;
    overflow: hidden;
}

.form .input-box {
    display: flex;
    align-items: center;
    height: 104rpx;
    /* margin: 0 40rpx; */
    border-bottom: 1px solid rgba(51, 51, 51, 0.2);
}

.form .input-box .phone {
    width: 19rpx;
    height: 28rpx;
    margin-right: 31rpx;
}

.form .input-box .identity {
    width: 29rpx;
    height: 23rpx;
    margin-right: 21rpx;
}

.form .input-box .text {
    font-family: "PingFangSC-Light";
    font-size: 30rpx;
    letter-spacing: 1px;
    color: #484848;
    margin-right: 21rpx;
}

.form .input-box input {
    flex: 1;
    font-family: "PingFangSC-Light";
    font-size: 30rpx;
}

.form .input-box .getCode {
    width: 135rpx;
    height: 37rpx;
    line-height: 37rpx;
    padding: 0;
    border-radius: 8rpx;
    white-space: nowrap;
    font-family: "PingFangSC-Regular";
    font-size: 18rpx;
    background-color: #ffffff;
    color: #e84358;
}

.form button::after {
    border: solid 1px #e84358;
}

.form .button-box {
    padding: 50rpx 0;
    text-align: center;
}

.form .button-box button {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 40rpx;
    font-size: 28rpx;
    border: none;
    display: inline-block;
    font-family: "PingFangSC-Regular";
    box-shadow: 0 8rpx 13rpx rgba(235, 97, 108, 0.42);
}

.form .button-box button.login {
    background-color: #e84358;
    color: #ffffff;
}

.rule {
    text-align: center;
    padding-top: 50rpx;
    line-height: 26rpx;
}

.rule-title {
    font-size: 32rpx;
    color: rgba(51, 51, 51, 1)
}

.rule-content {
    color: rgba(51, 51, 51, 1);
    font-size: 20rpx;
}



.success-modal {
    display: flex;
    position: fixed;
    align-items: center;
    justify-content: center;
    left: 0;
    top: 0;
    bottom: 0;
    right: 0;
    background: rgba(0, 0, 0, 0.678);
    z-index: 999;
}

.success-modal-container {
    width: 530rpx;
    overflow: hidden;
    /* height: 678rpx; */
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
}

.success-modal-wrap {
    background-color: #ffffff;
    border-radius: 12rpx;
    height: 608rpx;
    overflow: hidden;
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 48rpx;
}

.modal-coupon-image {
    display: block;
    width: 530rpx;
    height: 240rpx;
    margin: 0 auto;
}

.content-container {
    padding-top: 35rpx;
    text-align: center;
    padding-bottom: 60rpx
}

.success-title {
    font-size: 38rpx;
}

.success-content {
    font-size: 30rpx;
    color: rgba(51, 51, 51, 1);
    padding-top: 10rpx;
}

.tap-look-button {
    text-align: center;
    width: 414rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 40rpx;
    background-color: rgba(232, 67, 88, 1);
    background: linear-gradient(right, rgba(232, 67, 88, 1),rgba(238, 145, 142, 1)); /* 标准语法 */
    background: -moz-linear-gradient(right, rgba(232, 67, 88, 1),rgba(238, 145, 142, 1)); /* 标准语法 */
    background: -webkit-linear-gradient(left,rgba(232, 67, 88, 1),rgba(238, 145, 142, 1)); /* 标准语法 */
    background: -o-linear-gradient(right, rgba(232, 67, 88, 1),rgba(238, 145, 142, 1)); /* 标准语法 */
    color: #fff;
    box-shadow: 0 3rpx 18rpx rgba(232, 67, 88, 0.44);
}

.success-modal-close::after,
.tap-look-button::after {
    border: none
}

.modal-close-image {
    width: 42rpx;
    height: 42rpx;
    display: block;
}

.success-modal-close {
    background: none;
}



