.page-scroll-view{
  background: #fff;
  padding-bottom: 10rpx;
}
.height250rpx{
  height: 250rpx;
}
.height550rpx{
  height: 550rpx;
}
.height180rpx {
  height: 180rpx;
}
.height160rpx {
  height: 160rpx;
}
.search{
  width: 100%;
  background: #fff;
  top: 0;
}
.search-type-warp {
  width: 650rpx;
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  background: white;
  margin: 20rpx auto 10rpx auto;
  font-size: 30rpx;
  letter-spacing: 1px;
  color: #484848;
}
.search-type-warp.space-around {
  justify-content: space-around;
}
.search-type-warp .item {
  font-family: PingFangSC-Regular;
  color: #484848;
  padding-right: 20rpx;
  display: flex;
  flex-flow: row nowrap;
  align-self: center;
  display: block;
  background: url('https://oss.localhome.cn/new_icon/more1.png') no-repeat;
  background-position: 100% center;
  background-size: 14rpx 8rpx;
}
.house-header {
  position: fixed;
  left: 0;
  right: 0;
  z-index:99;
  padding-bottom: 20rpx;
  background: white;
}
.search-bar-wrapper{
  display: flex;
  justify-content: space-between;
}
.search-map-btn{
  padding: 0 10px;
  display: flex;
  justify-content: center;
  align-items: center;
}
.search-map-btn image{
  width: 28px;
  height: 28px;
  display: block;
}
.searchbar-result{
    margin-top: 0;
    font-size: 14px;
}
.searchbar-result:before{
    display: none;
}
.weui-cell{
    padding: 12px 15px 12px 35px;
}
.search-bg__color{
  width: 100%;
  order: 0;
  background: #fff;
  border:0;
}
.search-bar__none_border{
  border: 0;
}
.search-bar-bg__color{
  background: #F5F5F5;
  border-radius: 50px;
}
.screen{
  font-size: 13px;
  background: #fff;
}
.screen-items{
  display: flex;
}
.scope{
  max-width: 25%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  text-align: center;
  position: relative;
}
.screen-item1::after,.screen-item2::after{
  content: "";
  position: absolute;
  right: 5px;
  bottom:10px;
  width: 0;
  height: 0;
  border-width: 3px;
  border-style:solid;
  border-color:  transparent #747474 #747474 transparent;
 }

 .navbar{
  width: 560rpx;
  box-sizing: border-box;
  overflow: hidden;
  white-space: nowrap;
  margin-top: 10rpx; 
  display: inline-block;
}
.navbar-tab-item{
  text-align: center;
  min-width: 150rpx;
  height: 40rpx;
  line-height: 40rpx;
  margin:10rpx 30rpx 10rpx 0rpx;
  display: inline-block;
  background-color: #ffffff;
	box-shadow:0px 2rpx 13rpx 0px rgba(84,84,84,0.15);
	border-radius: 20rpx;
  font-size: 24rpx;
  color: #121212;
  border:1rpx solid rgba(255,255,255,1);
  transition: all 0.4s;
}
.styleTag{
  display: inline-block;
  width: 150rpx;
  overflow: hidden;
  background: #fff;
  white-space: nowrap;
  margin:10rpx 0rpx 10rpx 20rpx;
}
.transparent{
  border:1rpx solid rgba(255,255,255,1);
  box-shadow:0rpx 2rpx 13rpx 0rpx rgba(84,84,84,0.15);
  color:rgba(255,255,255,1);
  background-color: rgba(255,255,255,0)
}
.styleTag .tagMore{
  width:12rpx;
  height:6rpx;
  margin-left: 5rpx;
  display: inline-block;
  margin-bottom: 5rpx;
}
.navbar-tab-item:first-child {
  margin-left: 30rpx;
}

.hot-image {
  width: 30rpx;
  height: 24rpx;
  margin-top: 8rpx;
  vertical-align: top;
}
.main-style {
  color: #E84358;
}
.main-style.active {
  background: rgba(232,67,88,0.25);
  border: 1rpx solid rgba(232,67,88,0.02);
}
.active{
  color: #e84358;
  box-shadow: #e84358;
}
.transparent.active{
  /* border:1rpx solid #e84358; */
  background-color: rgba(255,255,255,1);
  color: rgba(0,0,0,1);
}
.transparent.styleTagActive{
  color: rgba(0,0,0,1);
  background-color: rgba(255,255,255,1);
}
.styleTagActive{
  color: #e84358;
  box-shadow: #e84358;
}
.content{
  padding: 10px;
  margin-bottom: 10px;
  background: #fff;
  position: relative;
}
.house-image {
  display: block;
  margin: 0 auto;
  position: relative;
  z-index: 2;
  width: 667rpx;
	height: 400rpx;
  border-radius: 20rpx;
  overflow: hidden;
  margin-bottom: 29rpx;
}

.house-image image {
  position: absolute;
  z-index: 1;
  bottom: 0;
  width: 100%;
}

/** 房源列表信息 **/
.house-info {
  display: flex;
  flex-flow: row nowrap;
  padding: 0 27rpx;
}

.house-info .left {
  flex-basis: 80%;
}

.tags {
  font-size: 0;
  width: 660rpx;
  padding: 0 27rpx;
  margin-bottom: 5rpx;
  display: flex;
  flex-flow: row nowrap;
  white-space: nowrap;
}

.tags .text{
  display: inline;
  line-height: 40rpx;
  font-size: 24rpx;
	letter-spacing: .4px;
  color: #e84358;
  margin-right: 20rpx;
}

.house-info .left .title {
  font-weight: 500;
	font-size: 36rpx;
  line-height: 42rpx;
	letter-spacing: 1rpx;
	color: #484848;
  display: block;
  display: -webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  text-overflow: ellipsis;
}

.house-info .left .price {
	font-size: 30rpx;
	line-height: 54rpx;
	letter-spacing: 1rpx;
  font-weight: 400;
	color: #555555;
}

.house-info .left .number-info {
  display: flex;
	font-size: 22rpx;
  font-weight: 300;
  color: #555555;
  margin-top: -5rpx;
}
.house-info .left .stars-container {
  display: inline;
  margin-right: 22rpx;
}
.house-info .left .star {
  margin-top: -6rpx;
  margin-right: 5rpx;
}
.house-info .right {
  flex-basis: 20%;
  display: flex;
  justify-content: flex-end;
}

.house-info .right image {
  width: 80rpx;
	height: 80rpx;
  border-radius: 100%;
  margin-top: 18rpx;
}

/** 房源列表信息 **/
