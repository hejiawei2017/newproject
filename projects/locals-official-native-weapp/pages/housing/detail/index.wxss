/* pages/housing/detail/index.wxss */
.page{
  padding-bottom: 128rpx;
}
.detail-content{
  margin: 25rpx 0rpx 0 0rpx;
}
.tab-wrapper{
  display: flex;
  white-space: nowrap;
  margin-left: 50rpx;
}
.tab-item{
  border:1px solid rgba(229,229,229,1);
  border-radius:6rpx;
  font-size: 20rpx;
  line-height: 20rpx;
  font-family:PingFangSC-Regular;
  color:rgba(153,153,153,1);
  margin-right: 26rpx;
  font-weight: 600;
  padding: 7rpx 10rpx;
}
.describe{
  margin: 28rpx 50rpx 0 50rpx;
	font-size: 42rpx;
	color: #333333;
  font-weight: bold;
  letter-spacing: 1rpx;
  line-height: 59rpx;
  padding: 0 !important;
}
.section{
  position: relative;
  padding: 50rpx 50rpx 100rpx 50rpx;
}
.safeguard-icons{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  margin-top: 55rpx;
}
.safeguard-icon-item{
  width: 25%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  font-size: 24rpx;
  color: #555555;
}
.safeguard-icon-item-tips{
  font-size: 18rpx;
}
.safeguard-icon-item:nth-child(1),.safeguard-icon-item:nth-child(2),.safeguard-icon-item:nth-child(3),.safeguard-icon-item:nth-child(4){
  margin-bottom: 60rpx;
}
.safeguard-icon-item image{
  width: 38rpx;
  margin-bottom: 20rpx;
  display: block;
}
.safeguard-icon-item text{
  width: 100%;
  height: 40rpx;
  display: inline-block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: center;
}
.detail-title{
  font-size:42rpx;
  font-family:PingFangSC-Semibold;
  font-weight:bold;
  color:rgba(51,51,51,1);
}
.base-info-icons{
  display: flex;
  flex-wrap: wrap;
  margin-top: 38rpx;
}
.base-info-icon-item{
  display: flex;
  align-items: center;
}
.base-info-icon-item:nth-of-type(n-1) {
  width: 45%;
}
.base-info-icon-item:nth-of-type(2n-1) {
  width: 55%;
}
.base-info-icon-item:nth-child(1),.base-info-icon-item:nth-child(2){
  margin-bottom: 30rpx;
}
.base-info-icon-item text{
  font-size: 30rpx;
	color: #555555;
  margin-left: 26rpx;
}
.base-info-icon-item image{
  width: 50rpx;
  height: 50rpx;
  display: block;
}
/* 房东模块 */
.landlord-wrap {
  margin-top: 50rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.landlord-photo{
  margin: 0 12rpx 0 0;
}
.landlord-photo image{
  width: 96rpx;
  height: 96rpx;
  overflow: hidden;
  border-radius: 50%;
  display: block;
  background: #333;
}
.landlord-name {
  flex: 1;
}
.landlord-name .nickName{
  font-size:32rpx;
  font-family:PingFangSC-Medium;
  font-weight:bold;
  color:rgba(72,72,72,1);
}
.landlord-name .label {
  font-size:24rpx;
  font-family:PingFangSC-Medium;
  font-weight:bold;
  color:rgba(153,153,153,1);
}

.contact-landlord-button {
  display: flex;
  align-items: center;
}

.contact-landlord-button image {
  width:24rpx;
  height:23rpx;
  margin-right: 14rpx;
}

.contact-landlord-button text{
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  color:rgba(232,67,88,1);
}
.landlord-describe,.deposit-wrapper{
  font-size: 28rpx;
	color: #555555;
  margin-top: 44rpx;
}
.landlord-more {
  margin-top: 36rpx;
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  color:rgba(232,67,88,1);
}
/* 房东模块 end */
