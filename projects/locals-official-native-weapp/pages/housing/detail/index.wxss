/** pre-index **/
.full-screen {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: #5c5d63;
  z-index: 99999;
}

.mask-under-button {
  display: flex;
  justify-content: space-between;
