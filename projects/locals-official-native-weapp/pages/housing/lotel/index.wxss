  margin: 20rpx 20rpx 0 0;
}
.house-item .info .title {
  display: flex;
  align-items: center;
}
.house-item .info .title > .short-title {
  display: inline-block;
  width: 380rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 36rpx;
	letter-spacing: 0.7px;
  color: #333333;
  font-weight: 500;
}
.house-item .info .title > .chart {
  display: flex;
  align-items: center;
  white-space: nowrap;
  font-family: PingFangSC-Regular;
