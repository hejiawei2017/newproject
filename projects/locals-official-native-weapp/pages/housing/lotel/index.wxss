}

super-image.lotel-logo image {
  width: 258rpx;
	height: 60rpx;
}

.main-container .main-info {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  height: 364rpx;
  background: white;
  border-radius: 60rpx 0 0 0;
  box-sizing: border-box;
  padding: 40rpx 28rpx;
}

.main-container .main-info .store-name {
  font-family: PingFangSC-bold;
  font-size: 48rpx;
	line-height: 48rpx;
  letter-spacing: 1px;
  font-weight: 700;
  color: #333333;
  margin-top: 18rpx;
}

.main-container .main-info .tags {
  margin-top: 8rpx;
  white-space: nowrap;
}

.main-container .main-info .tags .item {
  display: inline-block;
  font-size: 22rpx;
  padding: 4rpx 8rpx;
  margin-right: 10rpx;
  text-align: center;
	letter-spacing: 0.4px;
  color: #999999;
  background: #f5f5f5;
  border-radius: 4px;
}

.line-container {
  padding: 50rpx 28rpx;
  position: relative;
}

.comment-container {
  height: 36rpx;
  margin-top: 50rpx;
  display: flex;
  justify-content: space-between;
}

.comment-container > .stars {
  display: flex;
  align-items: center;
}

.comment-container > .stars > text {
  display: inline-block;
  font-family: PingFangSC-Semibold;
	font-size: 36rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0.7px;
	color: #333333;
}

.comment-container > .stars stars {
  height: 22rpx;
}

.comment-container > .stars stars icon {
  width: 24rpx;
	height: 22rpx;
}

.comment-container .comment {
  display: flex;
  font-family: PingFangSC-Regular;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0.5px;
	color: #333333;
}

.comment-container .comment .avatars {
  white-space: nowrap;
}

.comment-container .comment .avatars image {
  display: inline-block;
  width: 59rpx;
	height: 59rpx;
	background-color: #333333;
  border: solid 2px #ffffff;
  border-radius: 100%;
  transform: translateY(-25%);
  margin-right: -15rpx;
}

.comment-container .comment .avatars image:last-child {
  margin: 0 !important;
}

.landlord-container .avatar-info {
  display: flex;
  align-items: center;
}

.landlord-container .avatar-info image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 100%;
}

.landlord-container .avatar-info > view {
  font-size: 32rpx;
	line-height: 32rpx;
	letter-spacing: 0.6px;
  color: #484848;
  margin-left: 27rpx;
}

.landlord-container .landlord-intro {
  margin-top: 47rpx;
}
/*  日期容器 start */
.draw-date-wrapper{
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-family: PingFangSC-Regular;
}
.draw-date-center{
  width: 85rpx;
  display: flex;
  align-items: center;
}
.draw-date-center text:nth-child(1){
  width: 85rpx;
	height: 31rpx;
  line-height: 31rpx;
	border-radius: 16rpx;
	border: 1px solid #333333;
  font-size: 24rpx;
  color: #333333;
  text-align: center;
}
.draw-date-center text:nth-child(2){
  font-size: 32rpx;
	color: #333333;
  display: block;
  margin-top: 10rpx;
}
.draw-date-tips{
	font-size: 26rpx;
	letter-spacing: 0.5px;
	color: #333333;
}
.draw-date-info{
  font-size: 36rpx;
	letter-spacing: 0.7px;
	color: #484848;
  display: flex;
  align-items: center;
}
.draw-date-info text:nth-child(2){
  font-size: 24rpx;
	color: #555555;
  margin-left: 16rpx;
}
/*  日期容器 end */

/* 筛选房源 start */
.filter-house {
  padding-top: 0;
}
.filter-house .content {
  margin-top: 50rpx;
}
.filter-house .tabs-wrap {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.filter-house .tabs-wrap > scroll-view {
  width: 600rpx;
}

.filter-house .tabs-wrap > scroll-view .realy-scroll-wrap {
  display: flex;
  flex-flow: row nowrap;
  align-content: center;
  box-sizing: border-box;
}

.filter-house .tabs-wrap > scroll-view .realy-scroll-wrap > view {
  width: 114rpx;
  margin-right: 17rpx;
  border-radius: 20rpx;
  height: 40rpx;
  line-height: 38rpx;
  text-align: center;
  font-size: 20rpx;
  border: solid 1px #dcdcdc;
  color: #333333;
  background: white;
}

.filter-house .tabs-wrap > scroll-view .realy-scroll-wrap > view.active {
  background-color: #8a1353;
  border: solid 1px #8a1353;
  color: #ffffff;
}

.filter-house .tabs-wrap > scroll-view .active {
  color: #333333;
  background-color: #f0f0f0;
}

.filter-house .tabs-wrap > .filter-button {
  font-size: 28rpx;
	color: #8a1353;
}

/* 筛选房源 end */

/* 房源卡片 start */
.house-item {
  display: flex;
  justify-content: space-between;
  margin: 40rpx 0 0 0;
  height: 240rpx;
	background-color: #ffffff;
  border-radius: 8rpx;
  overflow: hidden;
  padding: 0 !important;
}
.house-item .house-image {
  position: relative;
  width: 202rpx;
	height: 100%;
	background-color: #333333;
  overflow: hidden;
}
.house-item .house-image image {
