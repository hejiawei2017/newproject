.main-container {
  height: 100vh;
}

.main-title {
  display: flex;
  align-items: center;
  font-family: PingFangSC-Semibold;
	font-size: 40rpx;
	letter-spacing: 0.8px;
  color: #484848;
  margin-bottom: 40rpx;
}

.sub-title {
  font-family: PingFangSC-Semibold;
	font-size: 30rpx;
	letter-spacing: 0.6px;
	color: #333333;
}

.sub-content {
  display: block;
  margin: 30rpx 0 60rpx 0;
  font-family: PingFangSC-Regular;
	font-size: 30rpx;
	line-height: 44rpx;
	letter-spacing: 0.6px;
	color: #999999;
}

.default-box-shadow {
	box-shadow: 0px 2px 27rpx 0px 
