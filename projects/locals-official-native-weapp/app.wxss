button::after {
  border: none;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.fullWidth {
    width: 100%;
}
/**
 *
 **/
.list-warpper {
  position: relative;
  width: 100%;
  height: 100vh;
  box-sizing: border-box;
}

.list-scroll {
  width: 100%;
  height: 100vh;
  box-sizing: border-box;
}

.border-after::after, .border-before::before {
    content: '';
    position: absolute;
    background-color: #ebebeb;
    display: block;
    z-index: 15;
    top: auto;
    right: auto;
    bottom: 1px;
    left: 0;
    height: 1px;
    width: 100%;
    transform-origin: 50% 100%;
    transform: scaleY(.8);
}

.border-before::before{
  top: 0;
  bottom: auto;
}
.list-scroll.top{
  padding-top: 90rpx;
}
/**
 * 推荐房源
 **/
 .scroll-recommend-housing{
    position: relative;
    width: 100%;
    box-sizing: border-box;
    overflow: hidden;
    background: #fff;
    white-space: nowrap;
 }
 .scroll-recommend-wrapper{
   width: 75vw;
   display: inline-block;
   margin-right: 20px;
 }
 .scroll-recommend-image{
    height: 350rpx;
    border-radius: 5px;
    overflow: hidden;
 }
.scroll-recommend-image image{
  width: 100%;
  height: 100%;
  display: block;
}
.scroll-recommend-housing-detail{
  display: flex;
  justify-content: space-between;
  margin-top: 15px;
}
.scroll-recommend-housing-photo{
    width: 20%;
    display: flex;
    align-items: center;
}
.scroll-recommend-housing-photo image{
  width: 35px;
  height: 35px;
  border-radius: 50%;
  border: 1px solid #d9d9d9;
}
.scroll-recommend-housing-base{
  width: 80%;
  display: flex;
  flex-direction: column;
}
.scroll-recommend-housing-base view:nth-child(1){
  font-weight: bold;
  font-size: 14px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.scroll-recommend-housing-base-right{
  font-size: 14px;
  display: flex;
  align-items: center;
}
.scroll-recommend-housing-base-right text{
  margin-right: 5px;
}
.scroll-recommend-housing-base-right image{
  width: 18px;
  height: 18px;
}

.map-mask {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: transparent;
}
.loader-line {
  margin: 200rpx auto;
  width: 50rpx;
  height: 120rpx;
  text-align: center;
  font-size: 10px;
  display:flex;
  justify-content:space-around;
}
.loader-line > view {
  background-color: #67CF22;
  height: 100%;
  width: 6rpx;
  display: inline-block;
  -webkit-animation: stretchdelay 1.2s infinite ease-in-out;
  animation: stretchdelay 1.2s infinite ease-in-out;
  border-radius:18rpx;
}

.loader-line .rect2 {
  -webkit-animation-delay: -1.1s;
  animation-delay: -1.1s;
}

.loader-line .rect3 {
  -webkit-animation-delay: -1.0s;
  animation-delay: -1.0s;
}

.loader-line .rect4 {
  -webkit-animation-delay: -0.9s;
  animation-delay: -0.9s;
}

.loader-line .rect5 {
  -webkit-animation-delay: -0.8s;
  animation-delay: -0.8s;
}

@-webkit-keyframes stretchdelay {
  0%,
  40%,
  100% {
      -webkit-transform: scaleY(0.4)
  }
  20% {
      -webkit-transform: scaleY(1.0)
  }
}

@keyframes stretchdelay {
  0%,
  40%,
  100% {
      transform: scaleY(0.4);
      -webkit-transform: scaleY(0.4);
  }
  20% {
      transform: scaleY(1.0);
      -webkit-transform: scaleY(1.0);
  }
}

.loader-dot {
  position: relative;
  width: 160rpx;
  margin: auto;
  display: flex;
  justify-content:space-around;
}
.loader-dot .dot {
  width: 24rpx;
  height: 24rpx;
  background: #FF6666;
  border-radius: 100%;
  display: inline-block;
  animation: slide 1s infinite;
}
.loader-dot .dot:nth-child(1) {
  animation-delay: 0.1s;
}
.loader-dot .dot:nth-child(2) {
  animation-delay: 0.2s;
}
.loader-dot .dot:nth-child(3) {
  animation-delay: 0.3s;
}
.loader-dot .dot:nth-child(4) {
  animation-delay: 0.4s;
}
.loader-dot .dot:nth-child(5) {
  animation-delay: 0.5s;
}

@-webkit-keyframes slide {
  0% {
    transform: scale(1);
  }
  50% {
    opacity: 0.3;
    transform: scale(2);
  }
  100% {
    transform: scale(1);
  }
}
@keyframes slide {
  0% {
    transform: scale(1);
  }
  50% {
    opacity: 0.3;
    transform: scale(2);
  }
  100% {
    transform: scale(1);
  }
}

