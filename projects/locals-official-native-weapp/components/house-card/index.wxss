.house-info {
  display: flex;
  flex-flow: column nowrap;
  padding: 20rpx 20rpx 10rpx 20rpx;
}

.tags {
  font-size: 0;
  width: 500rpx;
  margin: 0 0 0rpx 0rpx;
  white-space: nowrap;
}

.tags .text{
  display: inline-block;
  line-height: 20rpx;
  font-size: 20rpx;
  padding: 7rpx 8rpx;
	font-family:PingFangSC-Regular;
  color:rgba(51,51,51,1);
  margin-right: 20rpx;
  background:rgba(153,153,153,0.1);
  border-radius:4rpx;
  text-align: center;
}

.ellipsis {
  display: block;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  overflow: hidden;
  text-overflow: ellipsis;
}

.design-wrap {
  position: absolute;
  z-index: 9;
  top: 308rpx;
  left: 39rpx;
  height: 45rpx;
  background:rgba(51,51,52,0.8);
  border-radius:5px 5px 5px 5px;
  padding: 0rpx 20rpx;
  display: flex;
  align-items: center;
}
.design-by-image {
  width: 117rpx;
  height: 28rpx;
}
.design-name .image {
  width: 100%;
  height: 100%;
}
.designer {
  font-size: 18rpx;
  color: rgba(255,255,255,1);
  margin-left: 5px;
}

.house-info .title {
  font-weight: bold;
  font-size: 30rpx;
  margin: 20rpx 0;
  color: rgba(72,72,72,1);
}

.flex-space-between {
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
}

.house-info .title .lotel-star-text {
  color: #8A1353;
  font-family:PingFangSC-Medium;
  font-weight: bold;
  font-weight: 400;
}

.house-info .title .lotel-star-text::after {
  content: ' 分';
  font-family:PingFangSC-Regular;
  font-size: 24rpx;

}

.house-info .footer {
  display: flex;
  align-items: center;
  margin-bottom: 25rpx;
}

.house-info .footer .card-text {
  border: 1px solid #E84358;
  border-radius: 5rpx;
  background: white;
  color: #E84358;
  font-size: 20rpx;
  padding: 4rpx 10rpx;
  font-family:PingFangSC-Light;
  margin: 6rpx 0 0 16rpx;
}

.house-info .footer .card {
  height: 28rpx;
  line-height: 28rpx;
  font-size: 18rpx;
  font-family: PingFangSC-Light;
  font-weight: 300;
  color: white;
  border-radius: 28rpx;
  padding: 0 10rpx;
  text-align: center;
  margin-left: 10rpx;
}

.house-info .footer .card-image {
  width:20rpx;
  height:30rpx;
  margin: 2rpx 8rpx 0 0;
}

.house-info .footer .card.SILVER {
  background:linear-gradient(0deg, rgba(134,150,168,1) 0%,rgba(182,192,207,1) 100%);
  box-shadow:0px 6rpx 8rpx 0px rgba(151,151,151,0.41);
}

.house-info .footer .card.GOLD {
  background: linear-gradient(0deg, rgba(255,176,51,1) 0%, rgba(244,197,71,1) 100%);
  box-shadow: 0px 6rpx 8rpx 0px rgba(242,133,45,0.41);
}

.house-info .footer .card.BLACK {
  background:linear-gradient(0deg, rgba(35,35,35,1) 0%,rgba(91,91,91,1) 100%);
  box-shadow:0px 6rpx 8rpx 0px rgba(151,151,151,0.41);
}

.price {
  height: 30rpx;
  line-height: 30rpx;
  font-family: PingFangSC-Semibold;
  letter-spacing: 1px;
	font-size: 32rpx;
  font-weight: 300;
  color: rgba(232,67,88,1);
}

.price::before {
  content: '¥';
  font-size: 20rpx;
}

.price::after {
  content: '起';
  font-size: 20rpx;
}

.house-info .footer .origin-price {
  font-family: PingFangSC-Semibold;
  text-decoration: line-through;
  margin: 0rpx 0 0 10rpx;
  font-size:18rpx;
  color: #CCCCCC;
  letter-spacing: 1px;
  height: 30rpx;
  line-height: 30rpx;
}
.lotel-money {
  color: #8a1353 !important;
}
.house-info .number-info {
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
	font-size: 22rpx;
  font-weight: 300;
  color: #555555;
  margin-bottom: 15rpx;
}
.house-info .stars-container {
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  margin-right: 6rpx;
  height: 18rpx;
}
.house-info  .line {
  margin: 0 10rpx;

}
.comment-bold{
  font-size: 24rpx;
  font-weight: bold;
  margin-right: 25rpx;
}

.avatar {
  position: absolute;
  z-index: 2;
  top: 400rpx;
  right: 20rpx;
  transform: translateY(-50%);
}

.avatar .image {
  width: 80rpx;
	height: 80rpx;
  border-radius: 100%;
  margin-top: 18rpx;
  background: white;
}

.icon-star {
  background: url(https://oss.localhome.cn/new_icon/star.png) no-repeat;
  background-size: 100% 100%;
  width: 18rpx;
  height: 18rpx;
}

.icon-star1 {
  background: url(https://oss.localhome.cn/new_icon/starc1.png) no-repeat;
  background-size: 100% 100%;
  width: 18rpx;
  height: 18rpx;
}

.icon-like-off {
  background: url(https://oss.localhome.cn/localhomeqy/like-off.png) no-repeat;
  background-size: 100% 100%;
  width:41rpx;
  height:34rpx;
}

.icon-like-on {
  background: url(https://oss.localhome.cn/localhomeqy/like-on.png) no-repeat;
  background-size: 100% 100%;
  width:41rpx;
  height:34rpx;
}
.like-wrap {
  position: absolute;
  z-index: 4;
  top: 0;
  right: 0;
  padding: 10rpx 20rpx;
}

.location-distance {
  /* margin-left: 15rpx; */
  height: 32rpx;
  line-height: 32rpx;
}

.lotel-info {
  margin: 0 0 0 0rpx;
  display: flex;
  align-items: center;
}
.logo-image {
  width: 183rpx;
  height: 45rpx;
}
.ellipsis-text{
  float: right;
  color: #8A1353;
}
.bookedView{
  position: absolute;
  top: 0;
  left: 0;
  height:100%;
  /* background:rgba(0,0,0,0.63); */
  width: 100%;
  z-index:10;
}
.bookedImg{
  width:162rpx;
  height:162rpx;
  background:rgba(0,0,0,0.5);
  border-radius:50%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  color:#ffffff;
  font-size:24rpx;
  line-height:162rpx;
  text-align: center;
}
.bookedView image{
  width:129rpx;
  height:129rpx;
  border:5rpx solid rgba(240,240,240,1);
  border-radius:50%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  z-index:10;
}

.xiaji-icon {
  width: 75rpx;
  height: 75rpx;
  position: absolute;
  top: 0;
  left: 38rpx;
}
.lotelTags{
  width: 100%;
}
.hotScenicSpots{
  width: 220rpx;
  display: inline-block;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  vertical-align: top;
}