{
  "name": "vue-ssr",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "client:dev": "webpack-dev-server --config ./config/webpack.client.js",
    "client:build": "webpack --config ./config/webpack.client.js  --watch --mode=development",
    "server:build": "webpack --config ./config/webpack.server.js  --watch --mode=development",
    "client:noWatch": "webpack --config ./config/webpack.client.js  --mode=development",
    "server:noWatch": "webpack --config ./config/webpack.server.js  --mode=development",
    "client": "webpack --config ./config/webpack.client.js --mode=production",
    "server": "webpack --config ./config/webpack.server.js --mode=production",
    "dev": "npm-run-all client:noWatch server:noWatch appserver",
    "dev:watch": "npm-run-all --parallel client:build server:build",
    "appserver": "nodemon server.js",
    "build": "cross-env IS_BUILD=1 npm-run-all client server "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
