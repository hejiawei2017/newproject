{
  "name": "gtc-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:client": "cross-env NODE_ENV=production WEBPACK_TARGET=web vue-cli-service build",
    "build:server": "cross-env NODE_ENV=production WEBPACK_TARGET=node vue-cli-service build",
    "build:win": "npm run build:server && npm run build:client",
    "build:mac": "npm run build:server && mv dist/vue-ssr-server-bundle.json bundle && npm run build:client && mv bundle dist/vue-ssr-server-bundle.json",
    "start": "cross-env NODE_ENV=production node ./server/ssr.js",
    "lint": "vue-cli-service lint",
    "dev:serve": "cross-env WEBPACK_TARGET=node nodemon ./server/ssr.js",
    "dev": "concurrently \"npm run serve\" \"npm run dev:serve\" ",
    "settype": "node ./setSystemType.js",
    "server:start": "cross-env NODE_ENV=production WEBPACK_TARGET=node pm2-docker start  ./server/ssr.js  --name serverssr --watch",
    "server:stop": "pm2-docker stop serverssr"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.6.5",
    "echarts": "^4.9.0",
    "element-ui": "^2.15.2",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "jsdom": "^16.5.3",
    "koa": "^2.5.3",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "qrcodejs2": "0.0.2",
    "register-service-worker": "^1.7.1",
    "scrollreveal": "^4.0.9",
    "videojs-contrib-hls": "^5.15.0",
    "vue": "^2.6.12",
    "vue-awesome-swiper": "^3.1.3",
    "vue-count-to": "^1.0.13",
    "vue-fullpage.js": "^0.1.7",
    "vue-router": "^3.2.0",
    "vue-server-renderer": "^2.6.12",
    "vue-video-player": "^5.0.2",
    "vuex": "^3.4.0",
    "xss": "^1.0.9"
  },
